paths:
  package.json:
    location: /package.json
    variables:
      project-name:
        question: What should the project name be?
      project-version:
        question: What should the project's starting version be?
      project-description:
        question: Pleae provide a one line description for the project.
      author:
        question: Who is the author of the project?

  package-lock.json:
    location: /package-lock.json
    variables:
      project-name:
        question: What should the project name be? This value should be the same as what you gave for project-name previously.
  src/App.js:
    location: /src/App.js
    variables:
      hello-world-message:
        question: What do you want your hello world message to be?
